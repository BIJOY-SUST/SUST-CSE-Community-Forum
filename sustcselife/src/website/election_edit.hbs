                     * @param {type} message
                     * @param {type} icon
                     * @returns {undefined}
                     */
                    function showDesktopNotification(message, body, friendOrGroup, postKey, groupKey, postOwnerKey, newFriend, icon, sound, timeout) {
                        if (!timeout) {
                            timeout = 4000;
                        }
                        requestNotificationPermissions();
                        var instance = new Notification(
                            message, {
                            body: body,
                            icon: icon,
                            sound: sound
                        }
                        );
                        instance.onclick = function () {
                            // Something to do
                            // window.open('http://localhost:3005/home', '_blank');
                            openWindowWithPost(friendOrGroup, postKey, groupKey, postOwnerKey, newFriend);

                        };
                        instance.onerror = function () {
                            // Something to do
                        };
                        instance.onshow = function () {
                            // Something to do
                        };
                        instance.onclose = function () {
                            // Something to do
                        };
                        if (sound) {
                            instance.sound;
                        }
                        setTimeout(instance.close.bind(instance), timeout);
                        return false;
                    }

                    function openWindowWithPost(friendOrGroup, postKey, groupKey, postOwnerKey, newFriend) {
                        if (friendOrGroup === 'friend') {
                            var f = document.getElementById('newWindowFormFriend');
                            f.eachPostUniqueID.value = postKey;
                            f.postOwnerID.value = postOwnerKey;
                            window.open('', 'TheWindow');
                            f.submit();
                        }
                        else if (friendOrGroup === 'group') {
                            var f = document.getElementById('newWindowFormGroup');
                            f.eachPostUniqueID.value = postKey;
                            f.eachPostOwnerUniqueID.value = postOwnerKey;
                            f.groupUniqueID.value = groupKey;
                            window.open('', 'TheWindow');
                            f.submit();
                        }
                        else if (friendOrGroup === 'groupPost') {
                            var f = document.getElementById('newWindowFormGroupPost');
                            f.groupKeyValue.value = groupKey;
                            window.open('', 'TheWindow');
                            f.submit();
                        }
                        else if (friendOrGroup === 'groupRequest') {
                            var f = document.getElementById('newWindowFormGroupPost');
                            f.groupKeyValue.value = groupKey;
                            window.open('', 'TheWindow');
                            f.submit();
                        }
                        else if (friendOrGroup === 'follow') {
                            var f = document.getElementById('newWindowFormFriendProfile');
                            f.friendUserKeyValue.value = newFriend;
                            window.open('', 'TheWindow');
                            f.submit();
                        }
                    }
